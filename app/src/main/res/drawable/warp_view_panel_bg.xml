<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_selected="true">

        <shape>

            <stroke android:width="1dp" android:color="#F58939" android:dashWidth="3dp" android:dashGap="1dp" />
            <corners android:radius="3dip" />

        </shape>
    </item>
    <item android:state_selected="false">

        <shape>
            <stroke android:width="1dp" android:color="#000000" android:dashWidth="3dp" android:dashGap="1dp" />
            <corners android:radius="3dip" />
        </shape>
    </item>


</selector>